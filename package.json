{
  "name": "starpc",
  "version": "0.0.2",
  "description": "Streaming protobuf RPC service protocol over any two-way channel.",
  "license": "Apache-2.0",
  "author": {
    "name": "Aperture Robotics LLC.",
    "email": "support@aperture.us",
    "url": "http://aperture.us"
  },
  "contributors": [
    {
      "name": "Christian Stewart",
      "email": "christian@aperture.us",
      "url": "http://github.com/paralin"
    }
  ],
  "repository": {
    "url": "git@github.com:aperturerobotics/starpc.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json --outDir ./dist/",
    "check": "tsc",
    "codegen": "npm run gen",
    "format": "prettier --write './srpc/**/(*.ts|*.tsx|*.js|*.html|*.css)'",
    "gen": "make genproto",
    "test": "make test",
    "test:integration": "make integration",
    "lint": "npm run lint:go && npm run lint:js",
    "lint:go": "make lint",
    "lint:js": "eslint -c .eslintrc.js --ext .ts ./",
    "postinstall": "patch-package",
    "precommit": "npm run format"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/varint": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "esbuild": "^0.14.43",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "patch-package": "^6.4.7",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "ts-proto": "^1.115.4",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@libp2p/mplex": "^1.2.1",
    "event-iterator": "^2.0.0",
    "isomorphic-ws": "^4.0.1",
    "it-length-prefixed": "^7.0.1",
    "it-pipe": "^2.0.3",
    "it-pushable": "^3.0.0",
    "it-stream-types": "^1.0.4",
    "it-ws": "^5.0.2",
    "protobufjs": "^6.11.3",
    "rxjs": "^7.5.5",
    "ws": "^8.8.0"
  }
}
