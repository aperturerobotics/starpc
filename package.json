{
  "name": "starpc",
  "version": "0.3.6",
  "description": "Streaming protobuf RPC service protocol over any two-way channel.",
  "license": "MIT",
  "author": {
    "name": "Aperture Robotics LLC.",
    "email": "support@aperture.us",
    "url": "http://aperture.us"
  },
  "contributors": [
    {
      "name": "Christian Stewart",
      "email": "christian@aperture.us",
      "url": "http://github.com/paralin"
    }
  ],
  "main": "./dist/srpc/index.js",
  "types": "./dist/srpc/index.d.ts",
  "files": [
    "!**/*.tsbuildinfo",
    "Makefile",
    "dist/echo",
    "dist/srpc",
    "e2e",
    "echo",
    "go.mod",
    "go.sum",
    "integration",
    "patches",
    "srpc"
  ],
  "repository": {
    "url": "git@github.com:aperturerobotics/starpc.git"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc --project tsconfig.build.json --outDir ./dist/",
    "check": "tsc",
    "deps": "depcheck",
    "codegen": "npm run gen",
    "ci": "npm run build && npm run lint:js && npm run lint:go",
    "format": "prettier --write './{srpc,echo,e2e,integration}/**/(*.ts|*.tsx|*.html|*.css)'",
    "gen": "make genproto",
    "test": "npm run test:js && npm run test:go",
    "test:go": "make test",
    "test:js": "npm run build && cd e2e && esbuild e2e.ts --sourcemap --outfile=e2e.js --bundle --platform=node && node ./e2e.js",
    "test:integration": "make integration",
    "integration": "npm run test:integration",
    "lint": "npm run lint:go && npm run lint:js",
    "lint:go": "make lint",
    "lint:js": "eslint -c .eslintrc.js --ext .ts ./{srpc,echo}/**/*.ts",
    "prepare": "patch-package",
    "precommit": "npm run format"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "depcheck": "^1.4.3",
    "esbuild": "^0.14.47",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-proto": "^1.115.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@libp2p/interface-connection": "^2.1.0",
    "@libp2p/interface-stream-muxer": "^2.0.0",
    "@libp2p/mplex": "^3.0.0",
    "event-iterator": "^2.0.0",
    "isomorphic-ws": "^4.0.1",
    "it-length-prefixed": "^7.0.1",
    "it-pipe": "^2.0.3",
    "it-pushable": "^3.0.0",
    "it-stream-types": "^1.0.4",
    "it-ws": "^5.0.2",
    "long": "^5.2.0",
    "patch-package": "^6.4.7",
    "protobufjs": "^6.11.3",
    "rxjs": "^7.5.5",
    "ws": "^8.8.0"
  }
}
