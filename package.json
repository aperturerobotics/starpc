{
  "name": "starpc",
  "version": "0.15.3",
  "description": "Streaming protobuf RPC service protocol over any two-way channel.",
  "license": "MIT",
  "author": {
    "name": "Aperture Robotics LLC.",
    "email": "support@aperture.us",
    "url": "http://aperture.us"
  },
  "contributors": [
    {
      "name": "Christian Stewart",
      "email": "christian@aperture.us",
      "url": "http://github.com/paralin"
    }
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "!**/*.tsbuildinfo",
    "Makefile",
    "dist",
    "e2e",
    "echo",
    "go.mod",
    "go.sum",
    "integration",
    "patches",
    "srpc"
  ],
  "repository": {
    "url": "git@github.com:aperturerobotics/starpc.git"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc --project tsconfig.build.json --outDir ./dist/",
    "check": "npm run typecheck",
    "typecheck": "tsc --noEmit",
    "deps": "depcheck --ignores 'bufferutil,utf-8-validate,ts-proto,@aperturerobotics/ts-common'",
    "codegen": "npm run gen",
    "ci": "npm run build && npm run lint:js && npm run lint:go",
    "format": "prettier --write './{srpc,echo,e2e,integration,rpcstream}/**/(*.ts|*.tsx|*.html|*.css)'",
    "gen": "make genproto",
    "test": "npm run test:js && npm run test:go",
    "test:go": "make test",
    "build:e2e": "npm run build && cd e2e && esbuild e2e.ts --sourcemap --outfile=e2e.js --bundle --platform=node",
    "test:js": "npm run build:e2e && cd e2e && node ./e2e.js",
    "debug:js": "npm run build:e2e && cd e2e && node --inspect --inspect-brk ./e2e.js",
    "test:integration": "make integration",
    "integration": "npm run test:integration",
    "lint": "npm run lint:go && npm run lint:js",
    "lint:go": "make lint",
    "lint:js": "eslint -c .eslintrc.js --ext .ts ./{srpc,echo,rpcstream}/**/*.ts",
    "patch": "patch-package --use-yarn --patch-dir ./node_modules/@aperturerobotics/ts-common/patches",
    "prepare": "npm run patch && go mod vendor",
    "precommit": "npm run format"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@aperturerobotics/ts-common": "^0.2.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "bufferutil": "^4.0.7",
    "depcheck": "^1.4.3",
    "esbuild": "^0.15.16",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-proto": "^1.135.0",
    "typescript": "^4.9.3",
    "utf-8-validate": "^5.0.10"
  },
  "dependencies": {
    "@chainsafe/libp2p-yamux": "^3.0.3",
    "@libp2p/interface-connection": "^3.0.3",
    "@libp2p/interface-stream-muxer": "^3.0.1",
    "event-iterator": "^2.0.0",
    "is-promise": "^4.0.0",
    "isomorphic-ws": "^5.0.0",
    "it-first": "^2.0.0",
    "it-length-prefixed": "^8.0.3",
    "it-pipe": "^2.0.4",
    "it-pushable": "^3.1.0",
    "it-stream-types": "^1.0.4",
    "it-ws": "^5.0.3",
    "long": "^5.2.1",
    "patch-package": "^6.5.0",
    "protobufjs": "^7.1.2",
    "uint8arraylist": "^2.4.2",
    "ws": "^8.11.0"
  }
}
